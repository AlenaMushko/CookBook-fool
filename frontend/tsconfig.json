{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2019"],
//    "types": ["vitest/globals"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2019",
    "strict": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noEmit": true,


    "baseUrl": "./",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@pages/*": ["src/pages/*"],
      "@shared/*": ["src/shared/*"],
      "@types/*": ["src/types/*"],
      "@api/*": ["src/api/*"],
      "@routing/*": ["src/routing/*"],
      "@config/*": ["src/config/*"],
      "@utils/*": ["src/utils/*"],
      "@stores/*": ["src/stores/*"],
      "@apis/*": ["src/api/apis/*"],
      "@apiTypes/*": ["src/api/types/*"],
      "@messages/*": ["src/messages/*"],
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules", "dist", "build"],
}
